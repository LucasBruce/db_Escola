
=>ENTIDADES:

#ALUNO
#SERIE
#DISCIPLINA
#COORDENAÇÂO
#PROFESSOR
#TURMA
#HISTÒRICO


=>RELACIONAMENTOS:


#ALUNO(0:n) matriculado em uma (0:1)SERIE apenas;

#ALUNO cursa DISCIPLINA(n:n);

#HISTÒRICO traz DISCIPLINA(n:n);

#SERIE composta por DISCIPLINA(n:n);

#DISCIPLINA depende SERIE(n:n);

#SERIE(0:n) depende SERIE(0:n);

#DISCIPLINA(1:n) pertence COORDENAÇÂO(1:n);

#PROFESSOR vinculado COORDENAÇÂO(n:n);


